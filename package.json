{
    "name": "customasm-vscode",
    "displayName": "customasm-vscode",
    "description": "Support for the customasm syntax",
    "publisher": "hlorenzi",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.52.0"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/hlorenzi/customasm-vscode"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "customasm",
            "aliases": ["customasm"],
            "extensions": [".asm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "customasm",
            "scopeName": "source.customasm",
            "path": "./syntaxes/customasm.tmLanguage.json"
        }]
    }
}